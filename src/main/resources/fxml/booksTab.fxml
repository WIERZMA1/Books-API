<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.books.api.controller.gui.BooksTabController"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="booksTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               prefHeight="500.0" prefWidth="1500.0">
        <columns>
            <TableColumn fx:id="isbnColumn" prefWidth="75.0" text="isbn"/>
            <TableColumn fx:id="titleColumn" prefWidth="75.0" text="title"/>
            <TableColumn fx:id="subtitleColumn" prefWidth="75.0" text="subtitle"/>
            <TableColumn fx:id="bookRatingColumn" prefWidth="75.0" text="rating"/>
            <TableColumn fx:id="authorsColumn" prefWidth="75.0" text="authors"/>
            <TableColumn fx:id="categoriesColumn" prefWidth="75.0" text="categories"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>
